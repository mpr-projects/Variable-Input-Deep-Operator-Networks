{
    "rng_seed": 55,
    "n_runs": 5,

    "training_file": "data/training_data_grid.hdf5",
    "format_training_file": "default",

    "validation_file": "data/validation_data_grid.hdf5",
    "format_validation_file": "default",

    "test_file": "data/test_data_grid.hdf5",
    "format_test_file": "default",

    "batch_size_training": 50,
    "batch_size_test": 100,

    "model_params": {
	"coord_in_dim": 2,
        "sensor_dim": 1,
        "encoded_branch_dim": 20,
	"inner_branch_dim": 256,
        "coord_out_dim": 3,
	"p": 400,
	"n_heads": 4,
	"branch_neurons_coord_enc": [20, 20, 20],
	"branch_neurons_sensor_enc": [20, 20, 20],
    	"branch_neurons_weights":[128, 128, 128, 128],
        "branch_neurons_values": [128, 128, 128, 128],
	"branch_neurons_combine": [256, 256, 256, 256],
        "trunk_neurons": [500, 500, 500, 500]
    },

    "optimizer_params": {
	"lr": 1e-4,
	"weight_decay": 1e-9
    },

    "lr_scheduler": "MultiStepLR",
    "lr_scheduler_params": {
	"milestones": [20000, 40000, 60000, 80000],
	"gamma": 0.5
    },

    "max_epochs": 100000
}

